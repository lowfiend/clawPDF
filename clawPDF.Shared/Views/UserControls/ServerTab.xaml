<UserControl x:Class="clawSoft.clawPDF.Shared.Views.UserControls.ServerTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:clawSoft.clawPDF.Shared.Views.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label Name="ServerApiUrlLabel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="392">api url:</Label>
        <TextBox  Text="{Binding CurrentProfile.ServerSetting.ApiUrl, Mode=TwoWay}" HorizontalAlignment="Left" Height="23" Margin="0,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="392"/>
    </Grid>
</UserControl>
